    "wrtc": "^0.0.61",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.0",
    "@types/winston": "^2.2.0",
    "banner-webpack-plugin": "^0.1.3",
    "chai": "^3.5.0",
    "ignore-loader": "^0.1.2",
